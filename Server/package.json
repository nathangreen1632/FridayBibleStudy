{
  "name": "server",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && node ./dist/server.js\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "db:migrate": "NODE_ENV=development sequelize-cli db:migrate",
    "db:undo": "NODE_ENV=development sequelize-cli db:migrate:undo",
    "db:reset": "NODE_ENV=development sequelize-cli db:migrate:undo:all && NODE_ENV=development sequelize-cli db:migrate",
    "db:seed": "NODE_ENV=development sequelize-cli db:seed:all",
    "db:seed:undo": "NODE_ENV=development sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "One Guy Productions",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "resend": "^3.5.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.15.5",
    "eslint": "^9.9.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
